{
  "name": "reuben-package-installer",
  "version": "1.0.0",
  "description": "A package installer to install node modules and their dependencies",
  "scripts": {
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "startnmon": "nodemon --exec babel-node tools/srcServer.js",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --require ignore-styles --reporter progress tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "author": "Reuben Ellis",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.16.2",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "fs-extra": "^3.0.1",
    "node-fetch": "^1.7.1",
    "promise-limit": "^2.4.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.2",
    "semver": "^5.3.0",
    "tar-fs": "^1.15.3",
    "tar-stream": "^1.5.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "colors": "^1.1.2",
    "compression": "^1.7.1",
    "cross-env": "^2.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-watch": "^2.1.14",
    "eventsource-polyfill": "^0.9.6",
    "expect": "^21.2.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.30.1",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.12.0",
    "localtunnel": "^1.8.3",
    "mocha": "^3.5.3",
    "nock": "^8.2.1",
    "nodemon": "^1.12.1",
    "npm-run-all": "^3.1.2",
    "open": "^0.0.5",
    "promise-limit": "^2.4.0",
    "react-addons-test-utils": "^15.6.2",
    "redux-immutable-state-invariant": "^1.2.4",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.2",
    "semver": "^5.3.0",
    "style-loader": "^0.19.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.6.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1"
  }
}
